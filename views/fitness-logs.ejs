<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Past Workouts</title>
  <link rel="stylesheet" href="styles.css">
</head>

<body>
  <div class="text-box">
    <h1>Past Workouts</h1>
    <p class="normal-text">
      <ul>
        <% fitness.forEach(function(dat) { %>
          Time: <%= dat.time%>
          <br>
          Type: <%= dat.type%>
          <br>
          Date: <%= dat.date%>
          <br>
      <% }); %>
      </ul>
    </p>
    <form method = "post" action = "/returnHome">
      <input type = "submit" value = "Back" class = "red">
    </form>
  </div>
 </body>
</html>