<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>View Logs</title>
  <link rel="stylesheet" href="styles.css">
</head>

<body>
  <div class="text-box">
    <h1>User Logs</h1>
    <p class="normal-text">
      <ul>
        <% log.forEach(function(dat) { %>
          User: <%= dat.user %>
          <br>
          Logged in on <%= dat.date%>
          <br>
          <br>
      <% }); %>
      </ul>
    </p>
    <form method = "post" action = "/returnHome">
      <input type = "submit" value = "Back" class = "red">
    </form>
  </div>
 </body>
</html>